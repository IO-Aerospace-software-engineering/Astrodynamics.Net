<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>disable</ImplicitUsings>
        <Nullable>disable</Nullable>
        
    </PropertyGroup>

    <ItemGroup>
      <None Remove="resources\libIO.SDK.so" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="resources\IO.SDK.dll" />
      <None Remove="resources\IO.SDK.exp" />
      <None Remove="resources\IO.SDK.lib" />
    </ItemGroup>

    <ItemGroup>
      <Content Include="..\..\SDK\cmake-build-release\IO.SDK\libIO.SDK.so">
        <Link>resources/libIO.SDK.so</Link>
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </Content>
      <Content Include="resources\IO.SDK.dll">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </Content>
      <Content Include="resources\IO.SDK.lib">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </Content>
    </ItemGroup>

</Project>
