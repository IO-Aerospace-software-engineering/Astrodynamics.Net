<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>disable</ImplicitUsings>
        <Nullable>disable</Nullable>
    </PropertyGroup>

    <ItemGroup>
      <None Remove="resources\libIO.SDK.so" />
    </ItemGroup>

    <ItemGroup>
      <Content Include="..\..\SDK\cmake-build-debug\IO.SDK\libIO.SDK.so">
        <Link>resources/libIO.SDK.so</Link>
      </Content>
      <Content Include="..\..\SDK\cmake-build-release\IO.SDK\IO.SDK.dll">
        <Link>resources/IO.SDK.dll</Link>
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </Content>
      <Content Include="..\..\SDK\cmake-build-release\IO.SDK\IO.SDK.exp">
        <Link>resources/IO.SDK.exp</Link>
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </Content>
      <Content Include="..\..\SDK\cmake-build-release\IO.SDK\IO.SDK.lib">
        <Link>resources/IO.SDK.lib</Link>
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </Content>
    </ItemGroup>

</Project>
