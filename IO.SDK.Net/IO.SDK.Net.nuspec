<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>IO.SDK.Net</id>
        <authors>Sylvain Guillet</authors>
        <copyright>Sylvain Guillet</copyright>
        <version>0.1.0-alpha</version>
        <title>.Net proxy to IO SDK</title>
        <icon>images\dragonfly-dark-trans.png</icon>
        <readme>docs\README.md</readme>
        <projectUrl>https://github.com/IO-Aerospace-software-engineering/SDK.Net</projectUrl>
        <license type="expression">LGPL-3.0-or-later</license>
        <description>This package provides a .Net connector to IO.SDK library</description>
        <dependencies>
            <group targetFramework="net6.0"/>
        </dependencies>
        
    </metadata>
    <files>
        <!-- binding libraries -->
        <file src="bin/Release/net6.0/IO.SDK.Net.dll" target="lib/net6.0"/>
        <!-- native libraries -->
        <file src="resources/IO.SDK.dll" target="runtimes/windows10-x64"/>
        <file src="resources/IO.SDK.lib" target="runtimes/windows10-x64"/>
        <file src="resources/IO.SDK.exp" target="runtimes/windows10-x64"/>
        <file src="resources/libIO.SDK.so" target="runtimes/linux-x64"/>
        <!-- targets file -->
        <file src="IO.SDK.Net.targets" target="build/"/>
        <file src="resources/dragonfly-dark-trans.png" target="images/"/>
        <file src="../README.md" target="docs/"/>
    </files>
</package>